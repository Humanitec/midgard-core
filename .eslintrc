 {
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "func-names": "off",
    "arrow-body-style": "off",
    "consistent-return":"off",
    "prefer-const": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-param-reassign":"off",
    "no-console":"off"
  }
}
